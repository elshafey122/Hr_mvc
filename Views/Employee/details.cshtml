
@{
    ViewData["Title"] = "employee details";
}

<h1>employee details</h1>

<div class="card" style="width: 18rem;">
  <img src="data:image/jpeg;base64," class="card-img-top" alt="...">
    <ul class="list-group list-group-flush">
        <li class="list-group-item">name:  @Model.Name</li>
        <li class="list-group-item">Age:   @Model.Age</li>
        <li class="list-group-item">Salary @Model.Salary</li>
        <li class="list-group-item">Email:     @Model.Email</li>
        <li class="list-group-item">Password:   @Model.Password</li>
        <li class="list-group-item">Office:  @(Model.OfficeId!=null ? Model.office.Name: "no office" )</li>
      </ul>
      <a asp-action="index" >
      <button class="btn btn-danger" asp-action="index" asp-controller="Employee">back</button>
  </a>
</div>